# =============================================================
# DCG Admin Dashboard — Environment Variables
# =============================================================
# Copy this file to .env and fill in the values.
# NEVER commit .env to version control.
# =============================================================

# Backend API URL (required)
# Local development: http://localhost:3000/api
# Production: https://api.directconnectglobal.com/api
REACT_APP_API_URL=

# Google OAuth Client ID (required for Google Sign-In)
REACT_APP_GOOGLE_CLIENT_ID=

# OpenReplay — Session Replay (optional)
REACT_APP_OPENREPLAY_PROJECT_KEY=
REACT_APP_OPENREPLAY_INGEST_POINT=https://api.openreplay.com/ingest

# Application version (set by CI/CD)
REACT_APP_VERSION=1.0.0

